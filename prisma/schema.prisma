datasource db {
  provider = "sqlite"
  url      = env("DB_URL")
}
generator client {
  provider = "prisma-client-js"
}
model User {
  id       Int        @id @default(autoincrement())
  name     String?
  isWehp   Boolean    @default(false)
  Streepje Streepje[]
}

model Streepje {
  id     Int     @id @default(autoincrement())
  reason String?
  user   User    @relation(fields: [userId], references: [id])
  userId Int
}